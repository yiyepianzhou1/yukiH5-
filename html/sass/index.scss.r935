// 新品推荐 mixin
@mixin icon-recommend($bg1,
$bg2) {
    .nav-icon {
        justify-content: center;
        flex-wrap: nowrap;
        align-self: center;
        text-align: center;
        display: inline-flex;
        width: 48px;
        height: 48px; //border: 1px solid #D2D2D2; /*no*/
        background: url($bg1) no-repeat center;
        background-size: 100%;
        &:active {
            background: url($bg2) no-repeat;
            background-size: 100%;
        }
    }
}
// 两边都是图标的
@mixin icon-both-icon($bg1,
$bg2,$bg3,$bg4) {
    .nav-icon-left {
        justify-content: center;
        flex-wrap: nowrap;
        align-self: center;
        text-align: center;
        display: inline-flex;
        width: 48px;
        height: 48px; //border: 1px solid #D2D2D2; /*no*/
        background: url($bg1) no-repeat center;
        background-size: 100%;
        &:active {
            background: url($bg2) no-repeat;
            background-size: 100%;
        }
    }
    .nav-icon-right {
        justify-content: center;
        flex-wrap: nowrap;
        align-self: center;
        text-align: center;
        display: inline-flex;
        width: 48px;
        height: 48px; //border: 1px solid #D2D2D2; /*no*/
        background: url($bg3) no-repeat center;
        background-size: 100%;
        &:active {
            background: url($bg4) no-repeat;
            background-size: 100%;
        }
    }
}
@mixin icon-life($bg1,
$bg2) {
    .right-nav-icon {
        justify-content: center;
        flex-wrap: nowrap;
        align-self: center;
        text-align: center;
        display: inline-flex;
        width: 48px;
        height: 48px; //border: 1px solid #D2D2D2; /*no*/
        background: url($bg1) no-repeat;
        background-size: 100%;
        &:active {
