@charset "UTF-8";
@import 'functions';
body,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
p,
blockquote,
dl,
dt,
dd,
ul,
ol,
li,
pre,
figcaption,
figure,
fieldset,
legend,
button,
input,
textarea,
th,
td {
    margin: 0;
    padding: 0
}

input {
    border: 0;
    outline: none;
}

body,
button,
input,
select,
textarea {
    font: 12px/1 Lucida Grande, 'Microsoft YaHei', "Lucida Grande", "Lucida Sans Unicode", Helvetica, Arial, Verdana, sans-serif
}

h1 {
    font-size: 18px;
    font-weight: normal
}

h2 {
    font-size: 16px;
    font-weight: normal
}

h3 {
    font-size: 14px;
    font-weight: normal
}

h4,
h5,
h6 {
    font-size: 100%;
    font-weight: normal
}

address,
cite,
dfn,
em,
var {
    font-style: normal
}

code,
kbd,
pre,
samp,
tt {
    font-family: "Courier New", Courier, monospace
}

small {
    font-size: 12px
}

ul,
ol,
li {
    list-style: none
}

a {
    text-decoration: none
}

abbr[title],
acronym[title] {
    border-bottom: 1px dotted;
    cursor: help
}

q:before,
q:after {
    content: ''
}

legend {
    color: #000
}

fieldset,
img {
    border: 0
}

table {
    border-collapse: collapse;
    border-spacing: 0
}

hr {
    border: 0;
    height: 1px
}

* {
    -ms-word-break: break-all;
    word-break: break-all;
    -ms-word-wrap: break-word;
    word-wrap: break-word;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0)
}

header {
    width: 100%;
    position: fixed;
    top: 0;
    background: #ffffff;
}

footer {
    width: 100%;
    position: fixed;
    bottom: 0;
    background: #ffffff;
}

//  margin值 自动向上顶
main {
    margin-top: 88px;
    margin-bottom: 86px;
}

// mixin functions
@mixin header-nav-tab {
    padding: 0 32px;
    text-align: center; //width: 750px;
    /*no*/
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-between;
    align-items: center;
}

@mixin footer-bar {
    padding: 0 70px;
    text-align: center; //width: 750px;
    border: 1px solid #D2D2D2;
    /*no*/
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-between;
    align-items: center;
    .footer-bar-item {
        align-items: center;
        display: inline-flex; //width: 60px;
        //height: 60px;
        //border: 1px solid #D2D2D2; /*no*/
        flex-direction: column;
    }
}
// 定义icon-info()
/**
@ $num, eg: left right , 1 ,2,3,
*/

@mixin footer-bar-icon-info($num, $bg1, $bg2, $color) {
    .icon-#{$num} {
        justify-content: center;
        flex-wrap: wrap;
        align-self: center;
        text-align: center;
        display: inline-flex;
        width: 48px;
        height: 48px; //border: 1px solid #D2D2D2; /*no*/
        background: url($bg1) no-repeat;
        background-size: 100%;
        &:active {
            background: url($bg2) no-repeat;
            background-size: 100%;
        }
    }
    .info-#{$num} {
        display: inline-flex;
        justify-content: center;
        margin: 8px 0; //width: 60px;
        align-self: center;
        flex-wrap: nowrap;
        font-size: 20px; //border: 1px solid #D2D2D2; /*no*/
        &:active {
            color: $color;
        }
    }
}

// includes mixin
.footer-bar {
    //min-width: 750px;
    @include footer-bar;
    @include footer-bar-icon-info(1, "../img/tabbar_icon_home_n@3x.png", "../img/tabbar_icon_home_p@3x.png", #4595e5);
    @include footer-bar-icon-info(2, "../img/tabbar_icon_column_n@3x.png", "../img/tabbar_icon_column_p@3x.png", #4595e5);
    @include footer-bar-icon-info(3, "../img/tabbar_icon_bag_n@3x.png", "../img/tabbar_icon_bag_p@3x.png", #4595e5);
    @include footer-bar-icon-info(4, "../img/tabbar_icon_me_n@3x.png", "../img/tabbar_icon_me_p@3x.png", #4595e5);
}

// 定义图标的标准样式
@mixin icon-style {
    align-items: center;
    display: inline-flex;
    //width: 60px;
    flex-direction: column;
}

.header-nav {
    @include header-nav-tab;
    .header-nav-button-left {
        flex-grow:1;
        @include icon-style;
        @include footer-bar-icon-info('left', '../img/home_icon_ticket@3x.png', '../img/home_icon_ticket@3x.png', #4595e5);
    }
    .header-nav-button-right {
      flex-grow:1;
        @include icon-style;
        @include footer-bar-icon-info('left', '../img/home_icon_msg@3x.png', '../img/home_icon_msg@3x.png', #4595e5);
    }
    .search-input {
        flex-grow: 1;
        margin: 0 40px;
        width: 511px;
        height: 64px;
        border: 1px solid #d4d4d4;
        border-radius: 32px;
        display: flex;
        flex-direction: row;
        justify-content: left;
        align-self: center;
        .search-icon {
            display: inline-flex;
            align-self: center;
            margin-left: 16px;
            width: 32px;
            height: 32px;
            background: url("../img/nav_icon_search_small@3x.png") no-repeat;
            background-size: 100%;
        }
        .search {
            padding-left: 8px;
            display: inline-flex;
            align-self: center;
            .to-search {
                border: 0;
                font-size: 28px;
                display: inline-flex;
                align-self: center;
                justify-content: center;
                color: #333333;
                &::-webkit-input-placeholder {
                    color: #999999;
                }
            }
        }
    }
}

@mixin tab-nav-item($num,
$color) {
    .tab-nav-item-#{$num} {
        color: $color;
        flex-direction: row;
        justify-content: center;
        align-self: center;
        display: inline-flex; //width: 56px;
        height: 80px;
        font-size: 28px;
        align-items: center;
        border-bottom: solid 4px $color;
    }
}

