}

//  福利仓2级分类 弹出框
.popupBox-warehouse {
    // z-index:999;
    //overflow-y: scroll;
    //overflow-x: hidden;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, .5);
    @include align-items(row);
    align-items: flex-end;
    .popupBoxBody {
        width: 100%;
        background-color: #fff;
        position: relative;
        z-index:1;
        //height: 683px;
        // 价格范围标题
        .price-range-tilte {
            font-size: 26px;
            margin-top: 48px;
            margin-left: 32px;
            color: #666666;
        }
        // 输入价格范围
        .price-range {
            height: 64px;
            @include common-flex-direction-wrap(nowrap,
            row);
            margin-top: 32px;
            margin-left: 32px;
            .low-price,
            .high-price {
                display: flex;
                width: 210px;
                height: 64px;
                border: 1px solid #d9d9d9;
                flex-direction: row;
                align-items: center;
                padding-left: 24px;
                border-radius: 10px;
                .price-value {
                    width: 100%;
                    height: 100%;
                    border-radius: 10px;
                    align-self: center;
                    font-size: 24px;
                }
            }
            .inline-line {
                margin: 0 24px;
                height: 3px;
                width: 20px;
                background: #d9d9d9;
                overflow: hidden;
            }
        }
        // 关闭
        .popupBox-closeBtn {
            @include size(48px);
            position: absolute;
            right: 24px;
            top: 24px;
            @include bg("../img/shopping-bag/bag_icon_close@3x.png");
        }
        // 品牌范围标题
              .trademark-tilte {
            font-size: 26px;
            margin-top: 48px;
            margin-left: 32px;
            color: #666666;
        }

        // 品牌标签组
        .tag-group {
            max-height: 312px;
            overflow-y: scroll;
            height: 312px;
            overflow-x: hidden;
            -webkit-overflow-scrolling: touch;
            &::-webkit-scrollbar{
                height: 0;
            }
            flex-wrap: wrap;
            margin: 32px 32px 60px 12px;
            display: flex;
            flex-direction: row;
            justify-content: flex-start;
            //align-content: flex-end;
            // 品牌item
            .tag-item {
                &:active {
                    color: #4595e5;
                    border-color: #4595e5;
                }
                margin-bottom: 20px;
                margin-left: 20px;
                border: 1px solid #d9d9d9;
                background-color: #f0f0f0;
                border-radius: 6px;
                height: 64px;
                display: flex;
                flex-direction: row;
                justify-content: center;
                flex-wrap: nowrap;
                align-items: center;
                // 默认样式
                .tag-text {
                    font-size: 24px;
                    margin: 0 24px;
                    color: #999999;
                    &:active {
                        color: #4595e5;
                    }

                }
            }

        }
        // 确定按钮
        .control-button-group{
            display: flex;
            flex-direction: row;
            justify-content:center;
            align-items: center;
        }
        .popupBox-ensureBtn {
            flex-grow: 1;
            @extend .button-primary;
            border-radius: 0;
        }
        .popupBox-reset {
            flex-grow: 1;
            @extend .button-primary;
            border-radius: 0;
            background-color: #ffffff;
            color: #4595e5;
        }
    }
}
.hintPopBox {
    position: fixed;
    width: 100%;
    left: 0;
    top: 960px;
    z-index: -1;
    @include justify-content(row, center);
    .hintPop {
        height: 48px;
        padding: 0 18px;
        background: rgba(0, 0, 0, .5);
