                row); //小时
                .h {
                    font-size: 28px;
                    display: flex;
                    flex-direction: row;
                    justify-content: center;
                    align-items: center;
                    @include common-width-height(58px,
                    44px);
                    @include collective-buying-bgcl(#333333)
                } //分
                .m {
                    font-size: 28px;
                    @include common-width-height(58px,
                    44px);
                    @include collective-buying-bgcl(#333333)
                } //秒
                .s {
                    font-size: 28px;
                    @include common-width-height(58px,
                    44px);
                    @include collective-buying-bgcl(#333333)
                }
            } // 展示图
            .show-goods-picture {
                margin-top: 56px;
                margin-right: 33px;
                margin-left: 38px;
                @include common-width-height(300px,
                300px);
                @include background-image('../img/logo.golang.png')
            }
