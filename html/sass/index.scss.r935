//弹窗样式
.popupBox {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, .5);
    @include align-items(row);
    align-items: flex-end;
    .popupBoxBody {
        width: 100%;
        height: 500px;
        background-color: #fff;
        position: relative;
        .popupBox-closeBtn {
            @include size(48px);
            position: absolute;
            right: 24px;
            top: 24px;
            @include bg("../img/shopping-bag/bag_icon_close@3x.png");
        }
        .popupBox-ensureBtn {
            @extend .button-primary;
            position: absolute;
            bottom: 0;
            width: 100%;
            margin: 0;
        }
    }
}

