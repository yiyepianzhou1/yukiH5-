    @include common-flex-direction-wrap(nowrap,
    row);
    justify-content: space-between;
    border: 1px solid #d9d9d9;
    height: 80px;
    .type-one,
    .type-two,
    .type-three {
        @include common-flex-direction-wrap(nowrap,
        row);
        .tag-name {
